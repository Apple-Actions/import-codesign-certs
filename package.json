{
  "name": "import-codesign-certs",
  "version": "4.0.0",
  "private": true,
  "description": "GitHub Action for Importing Code-signing Certificates into a Keychain",
  "main": "lib/main.js",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "tsc --outDir lib",
    "format": "prettier --write src/",
    "format-check": "prettier --check src/",
    "lint": "eslint",
    "package": "ncc build --license licenses.txt",
    "all": "yarn format && yarn lint && yarn build && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apple-Actions/import-codesign-certs.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Itty Bitty Apps Ptr Ltd",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "~1.11.0",
    "@actions/exec": "~1.1.0",
    "tmp": "~0.2.0"
  },
  "devDependencies": {
    "@types/node": "~22.13.0",
    "@types/tmp": "~0.2.0",
    "@vercel/ncc": "~0.38.0",
    "eslint": "~9.22.0",
    "eslint-plugin-github": "~6.0.0",
    "prettier": "~3.5.0",
    "typescript": "~5.8.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
